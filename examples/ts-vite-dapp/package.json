{
  "name": "biolerplate-template",
  "private": true,
  "version": "0.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "move:init": "node ./scripts/init",
    "move:test": "node ./scripts/move/test",
    "move:compile": "node ./scripts/move/compile",
    "move:publish": "node ./scripts/move/publish",
    "move:upgrade": "node ./scripts/move/upgrade",
    "dev": "vite",
    "test": "npx aptos-workspace test",
    "test:ci": "npx aptos-workspace test --timeout 300000",
    "test:mocha": "env TS_NODE_PROJECT=\"tsconfig.testing.json\" mocha --require ts-node/register 'tests/**/*.ts'",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "_fmt": "prettier 'frontend/**/*.ts'",
    "fmt": "npm run _fmt -- --write"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.27.1",
    "@aptos-labs/wallet-adapter-react": "^3.6.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.383.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "vite-plugin-environment": "^1.1.3"
  },
  "devDependencies": {
    "@aptos-labs/aptos-cli": "^0.2.0",
    "@aptos-labs/workspace": "file:../../workspace",
    "@types/chai": "^4.3.19",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.16.2",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "chai": "^4.5.0",
    "dotenv": "16.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.11",
    "mocha": "^10.7.3",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.10",
    "tree-kill": "1.2.2",
    "ts-node": "latest",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vite-plugin-notifier": "^0.1.5"
  }
}
