{
  "name": "biolerplate-template",
  "private": true,
  "version": "0.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "move:init": "node ./scripts/init",
    "move:test": "node ./scripts/move/test",
    "move:compile": "node ./scripts/move/compile",
    "move:publish": "node ./scripts/move/publish",
    "move:upgrade": "node ./scripts/move/upgrade",
    "dev": "ts-node index.ts",
    "test": "env TS_NODE_PROJECT=\"tsconfig.testing.json\" npx aw",
    "build": "tsc",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "_fmt": "prettier 'frontend/**/*.ts'",
    "fmt": "npm run _fmt -- --write"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.26.0"
  },
  "devDependencies": {
    "@aptos-labs/workspace": "link:../../workspace",
    "@types/chai": "4",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.14",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "chai": "4",
    "dotenv": "16.3.1",
    "eslint": "^8.57.0",
    "prettier": "^3.3.2",
    "tree-kill": "1.2.2",
    "ts-node": "10.9.1",
    "typescript": "^5.2.2"
  }
}
